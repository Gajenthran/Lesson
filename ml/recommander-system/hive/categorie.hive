SELECT m.title, v.vote_average FROM
rs_movies m JOIN vote v ON (m.movieId = v.movieId)
JOIN genres g ON (v.movieId = g.movieId)
WHERE g.genres LIKE '%Romance%' AND v.vote_count > 500.0
ORDER BY v.vote_average DESC;